#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,j=2;
    cin>>n;
    for(i=n+1;i<1000;i++)
    {
   for(j=2;j<i;j++)
        {
   if(i%j==0)  
   { break;
            }
   }
   if(i==j||i==1)
            {
                cout<<i;
                break;
            }
    }
    return 0;
}
